{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "start node-api",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}\\node-api\\server.js"
    },
    {
      "type": "pwa-chrome",
      "request": "launch",
      "name": "Launch Chrome against localhost",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}"
  }

  ]
}
